@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix tm: <http://www.transmodel-cen.eu/model#> .

@base <http://example.com/base/> .

<#AuthorityMapping>
  a rr:TriplesMap;
    
  rml:logicalSource [ 
    rml:source "stream://authority.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/PublicationDelivery/dataObjects/ResourceFrame/organisations/Authority";	
  ];

  rr:subjectMap [ 
    rr:template "agencies/{@id}" 
  ]; 
 
   rr:predicateObjectMap [ 
    rr:predicate rdf:type;
	rr:object tm:Authority
  ]; 
  
  rr:predicateObjectMap [ 
    rr:predicate tm:name; 
    rr:objectMap [
      rml:reference "Name"
    ]
  ];
  
  rr:predicateObjectMap [ 
    rr:predicate tm:characterisedBy; 
    rr:objectMap [
      rr:parentTriplesMap <#ContactDetailsMapping>;	  
	  rr:joinCondition [
		rr:parent "@id";
		rr:child "@id";
	  ]      
    ]
  ].  
  
<#ContactDetailsMapping>  
  
  rml:logicalSource [ 
	rml:source "stream://authority.xml";
    rml:referenceFormulation ql:XPath;
    rml:iterator "/PublicationDelivery/dataObjects/ResourceFrame/organisations/Authority"; 
  ];
  
  rr:subjectMap [
	rr:termType rr:BlankNode
  ]; 
  
  rr:predicateObjectMap [ 
    rr:predicate rdf:type;
	rr:object tm:ContactDetails
  ]; 
  
  rr:predicateObjectMap [ 
    rr:predicate tm:url; 
    rr:objectMap [
      rml:reference "ContactDetails/Url"
    ]
  ].
  
